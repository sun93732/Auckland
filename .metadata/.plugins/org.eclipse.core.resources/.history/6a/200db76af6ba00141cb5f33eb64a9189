package com.eric.spring;

public class Triangle {

	private String type;
	private int height;
	
	public int getHeight() {
		return height;
	}

	public Triangle() {
		
	}
	public void draw() {
		System.out.println(getType() + " Triangle drawing....");
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}
}
