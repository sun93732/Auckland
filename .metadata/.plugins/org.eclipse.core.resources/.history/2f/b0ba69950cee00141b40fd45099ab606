package com.orion;

public class Solution2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int[] array = {5, 8, 11, 33, 69, 100};
		//int[] array = { 1, 2, 5, 9, 9 };
		int x = solution(array, 100);
	}

	static int solution(int[] A, int X) {
		int N = A.length;
		if (N == 0) {
			return -1;
		}
		int l = 0;
		int r = N - 1;
		while (l < r) {
			int m = (l + r) / 2;
			if (A[m] > X) {
				r = m - 1;
			} else {
				

				if (A[l] == X) {
					return l;
				}
				l = m + 1;
			}
		}
		return -1;
	}

}
